  RowBox[{"standardToTraditionalBoxes", ",", "HoldAllComplete"}], "]"}], "\n", 
 RowBox[{
  RowBox[{"standardToTraditionalBoxes", "[", "boxes_", "]"}], ":=", 
  RowBox[{
   RowBox[{"Function", "[", 
    RowBox[{"expr", ",", 
     RowBox[{"MakeBoxes", "[", 
      RowBox[{"expr", ",", "TraditionalForm"}], "]"}], ",", 
     "HoldAllComplete"}], "]"}], "@@", 
